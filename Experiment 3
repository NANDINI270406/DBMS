SQL> desc course;
 Name                                                                                Null?    Type
 ----------------------------------------------------------------------------------- -------- --------------------------------------------------------
 COURSE_ID                                                                           NOT NULL VARCHAR2(8)
 TITLE                                                                               NOT NULL VARCHAR2(100)
 DEPT_ID                                                                                      NUMBER(38)
 CREDITS                                                                                      NUMBER(38)

SQL> desc section;
 Name                                                                                Null?    Type
 ----------------------------------------------------------------------------------- -------- --------------------------------------------------------
 SECTION_ID                                                                          NOT NULL NUMBER(38)
 COURSE_ID                                                                                    VARCHAR2(8)
 SEC_ID                                                                                       NUMBER(38)
 SEMESTER                                                                                     VARCHAR2(6)
 YEAR                                                                                         NUMBER(38)
 BUILDING                                                                                     VARCHAR2(50)
 ROOM_NUMBER                                                                                  VARCHAR2(10)
 TIME_SLOT_ID                                                                                 VARCHAR2(4)

SQL> desc enrollment;
 Name                                                                                Null?    Type
 ----------------------------------------------------------------------------------- -------- --------------------------------------------------------
 ENROLLMENT_ID                                                                       NOT NULL NUMBER(38)
 STUDENT_ID                                                                          NOT NULL NUMBER(38)
 SECTION_ID                                                                          NOT NULL NUMBER(38)
 ENROLLMENT_DATE                                                                              DATE
 STATUS                                                                                       VARCHAR2(20)

SQL>  SELECT C.course_id,C.title, COUNT(E.student_id) AS enrolled_students FROM COURSE C LEFT JOIN SECTION S ON C.course_id = S.course_id LEFT JOIN ENROLLMENT E ON S.section_id= E.section_id GROUP BY C.course_id,C.title ORDER BY C.course_id;

